/* ==========================================================================
   Base

   Default settings
========================================================================== */
html {
  font-size: 16px;
}

/* ==========================================================================
   Alerts

   Default Bootstrap Alert boxes. Provide contextual feedback messages for typical user actions with the handful of available and flexible alert messages
========================================================================== */
.alert-success {
  color: #5e6919;
  border-color: #c8d1ba;
  background-color: #e6eed1;
}

.alert-info {
  color: #235461;
  border-color: #bbd0d7;
  background-color: #d1e8f0;
}

.alert-warning {
  color: #665e48;
  border-color: #e6dbb7;
  background-color: #fff4ca;
}

.alert-danger {
  color: #811211;
  border-color: #d6b4b4;
  background-color: #fcd3d2;
}

/* ==========================================================================
   Buttons

   Default Bootstrap and Mendix Buttons
========================================================================== */
.mx-link {
  padding: 0;
  color: #006fe6;
}
.mx-link a {
  color: inherit;
}

.btn.iconMdsp,
.mx-button.iconMdsp,
.mx-link.iconMdsp {
  font-family: "Siemens Sans Roman", "Siemens Sans Regular", sans-serif !important;
  font-size: 14px;
  padding-top: 6px;
  padding-bottom: 5px;
}
.btn.iconMdsp.btn-icon-top:before,
.mx-button.iconMdsp.btn-icon-top:before,
.mx-link.iconMdsp.btn-icon-top:before {
  display: block;
}
.btn.iconMdsp:before,
.mx-button.iconMdsp:before,
.mx-link.iconMdsp:before {
  font-size: 18px;
  vertical-align: middle;
}
.btn img,
.mx-button img,
.mx-link img {
  height: 18px;
  margin-top: -4px;
  margin-right: 0px;
}

/* ==========================================================================
   Modals

========================================================================== */
.modal-dialog {
  margin-top: 44px !important;
}
.modal-dialog .modal-content {
  height: calc(100% - 44px);
}
.modal-dialog .modal-content .modal-body {
  height: calc(100% - 50px) !important;
}

/* ==========================================================================
   Navigation
   
========================================================================== */
.mx-navigationtree {
  /* Every navigation item */
}
.mx-navigationtree .navbar-inner > ul > li > a img {
  width: 25px;
  height: auto;
  margin-right: 18px;
}

.region-sidebar .mx-navigationtree {
  /* Sub navigation item specific */
}
.region-sidebar .mx-navigationtree .navbar-inner > ul > li a:hover .caret,
.region-sidebar .mx-navigationtree .navbar-inner > ul > li a:focus .caret,
.region-sidebar .mx-navigationtree .navbar-inner > ul > li a.active .caret {
  border-top-color: #323232;
